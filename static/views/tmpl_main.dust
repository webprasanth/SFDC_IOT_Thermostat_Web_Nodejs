<HTML>

<HEAD>
  <TITLE>{+title/}</TITLE>
  <link rel="stylesheet" href="/slds/styles/salesforce-lightning-design-system.min.css"> 
  {+style_block/}
  {+script_block/}

</HEAD>

<BODY>
  <div class="slds">
    <div class="slds-page-header" role="banner">
      <div class="slds-grid">
        <div class="slds-col slds-has-flexi-truncate">
          <div class="slds-card__header slds-grid">
            <div class="slds-media slds-media--center slds-has-flexi-truncate">
              <div class="slds-media__figure">
                <svg aria-hidden="true" class="slds-icon slds-icon-standard-contact slds-icon--small">
                  <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/slds/icons/custom-sprite/svg/symbols.svg#custom5"></use>
                </svg>
              </div>
              <div class="slds-media__body">
                <p class="slds-page-header__title slds-truncate slds-align-middle">
                  <a href="/">IOT Smart Thermostat</a>
                </p>
              </div>
            </div>
          </div>
        </div>

      </div>

    </div>

    <div class="slds-grid slds-m-top--large slds-m-left--large">

    </div>

    <div class="slds-col--padded slds-size--1-of-1">
      {+body_content/}
    </div>

</BODY>

</HTML>